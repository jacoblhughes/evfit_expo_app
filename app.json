{
  "expo": {
    "name": "EvidentFitness",
    "slug": "evidentfitnessapp",
    "privacy": "unlisted",
    "orientation": "portrait",
    "version": "2.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "2",
      "supportsTablet": true,
      "bundleIdentifier": "com.jacoblhughes.evidentfitnessapp"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/shapes.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jacoblhughes.evidentfitnessapp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
